<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Mds.Biodata.Core" namespace="Mds.Biodata.Domain">
    <class name="Usuario" table="Usuario" >
    <id name="ID" type="System.Int32" column="IDUsuario">
        <generator class="identity"/>
    </id>
    <property name="Nombre" column="Nombre" type="System.String" not-null="true" length="100"/>
    <property name="Apellido" column="Apellido" type="System.String" not-null="true" length="100"/>
    <property name="Nick" column="Nick" type="System.String" not-null="true" length="100"/>
    <property name="Password" column="Password" type="System.String" not-null="true" length="100"/>
 </class>
</hibernate-mapping>
