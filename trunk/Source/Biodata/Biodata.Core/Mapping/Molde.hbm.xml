<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Biodata.Core" namespace="Biodata.Domain">
    <class name="Molde" table="Molde" >
    <id name="ID" type="System.Int32" column="idMolde">
        <generator class="assigned"/>
    </id>
    <property name="Descripcion" column="descripcion" type="System.String" not-null="false" />
    <bag name="Pacientes" table="MoldePaciente">
        <key column="idMolde" />
        <many-to-many class="Paciente" column="idPaciente" />
    </bag>
 </class>
</hibernate-mapping>
